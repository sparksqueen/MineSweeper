{
  "settings": {
    "react": {
      "version": "latest"
    }
  },
  "env": {
    "browser": true,
    "react-native/react-native": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": [
    "airbnb",
    "eslint:recommended",
    "plugin:react/recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react", "prettier", "jest", "react-native"],
  "rules": {
    "react/prop-types": 0
    // "import/no-unresolved": 0,
    // "import/prefer-default-export": 0,
    // "react/jsx-props-no-spreading": 0
  }
}
