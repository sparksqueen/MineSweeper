  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "eslint": "^7.31.0",
    "eslint-config-airbnb": "^18.2.1",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.1",
    "prettier": "^2.3.2"
  },
  "husky": {
    "hooks": {
